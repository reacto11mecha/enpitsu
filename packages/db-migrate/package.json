{
  "name": "@enpitsu/db-migrate",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": [
    "dist",
    "migrations/**"
  ],
  "bin": {
    "db-migrate": "./dist/index.js"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "push": "drizzle-kit push:pg",
    "generate": "drizzle-kit generate:pg",
    "build": "tsup src/index.ts --format esm",
    "migrate": "pnpm with-env tsx migrate/index.mts",
    "studio": "drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "drizzle-orm": "^0.44.3",
    "postgres": "^3.4.7",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@enpitsu/eslint-config": "workspace:^0.2.0",
    "@enpitsu/prettier-config": "workspace:^0.1.0",
    "@enpitsu/tsconfig": "workspace:^0.1.0",
    "dotenv-cli": "^9.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "catalog:",
    "pg": "^8.16.3",
    "prettier": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "catalog:"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@enpitsu/eslint-config/base"
    ]
  },
  "prettier": "@enpitsu/prettier-config"
}
